<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>VotingServer</display-name>
	<context-param>
		<description>Location to store uploaded file</description>
		<param-name>logo-upload</param-name>
		<param-value>C:\tmp\</param-value>
	</context-param>

	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.voting.servlet.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>com.voting.servlet.LogoutServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Requests</servlet-name>
		<servlet-class>com.voting.servlet.RequestServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Requests</servlet-name>
		<url-pattern>/requests</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>ReqDetails</servlet-name>
		<servlet-class>com.voting.servlet.ReqDetailsServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ReqDetails</servlet-name>
		<url-pattern>/reqDetails</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>GetImage</servlet-name>
		<servlet-class>com.voting.servlet.GetImageServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>GetImage</servlet-name>
		<url-pattern>/getImage</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Approve</servlet-name>
		<servlet-class>com.voting.servlet.ApproveServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Approve</servlet-name>
		<url-pattern>/approve</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Reject</servlet-name>
		<servlet-class>com.voting.servlet.RejectServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Reject</servlet-name>
		<url-pattern>/reject</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>parties</servlet-name>
		<servlet-class>com.voting.servlet.PartiesServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>parties</servlet-name>
		<url-pattern>/parties</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>addParty</servlet-name>
		<servlet-class>com.voting.servlet.AddPartyServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>addParty</servlet-name>
		<url-pattern>/addParty</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>deleteParty</servlet-name>
		<servlet-class>com.voting.servlet.DeletePartyServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>deleteParty</servlet-name>
		<url-pattern>/deleteParty</url-pattern>
	</servlet-mapping>




	<servlet>
		<servlet-name>electionList</servlet-name>
		<servlet-class>com.voting.servlet.ElectionListServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>electionList</servlet-name>
		<url-pattern>/electionList</url-pattern>
	</servlet-mapping>




	<servlet>
		<servlet-name>addElection</servlet-name>
		<servlet-class>com.voting.servlet.AddElectionServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>addElection</servlet-name>
		<url-pattern>/addElection</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>deleteElection</servlet-name>
		<servlet-class>com.voting.servlet.DeleteElectionServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>deleteElection</servlet-name>
		<url-pattern>/deleteElection</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>participants</servlet-name>
		<servlet-class>com.voting.servlet.ParticipantsListServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>participants</servlet-name>
		<url-pattern>/participants</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>addParticipant</servlet-name>
		<servlet-class>com.voting.servlet.AddParticipantServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>addParticipant</servlet-name>
		<url-pattern>/addParticipant</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>removeParticipant</servlet-name>
		<servlet-class>com.voting.servlet.RemoveParticipantServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>removeParticipant</servlet-name>
		<url-pattern>/removeParticipant</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>vote</servlet-name>
		<servlet-class>com.voting.servlet.VoteServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>vote</servlet-name>
		<url-pattern>/vote</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>resultList</servlet-name>
		<servlet-class>com.voting.servlet.ResultListServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>resultList</servlet-name>
		<url-pattern>/resultList</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>results</servlet-name>
		<servlet-class>com.voting.servlet.ResultsServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>results</servlet-name>
		<url-pattern>/results</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>reqKeys</servlet-name>
		<servlet-class>com.voting.servlet.RequestPrivateKeyServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>reqKeys</servlet-name>
		<url-pattern>/reqKeys</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>